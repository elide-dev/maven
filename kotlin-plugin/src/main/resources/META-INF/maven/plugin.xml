<?xml version="1.0" encoding="UTF-8"?>


<plugin>
  <name>elide-kotlin-maven-plugin</name>
  <description>Kotlin compiler replacement</description>
  <groupId>dev.elide</groupId>
  <artifactId>elide-kotlin-maven-plugin</artifactId>
  <version>1.0.0</version>
  <goalPrefix>kotlin</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>compile</goal>
      <description>Compiles kotlin sources</description>
      <requiresDependencyResolution>compile</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>compile</phase>
      <implementation>dev.elide.maven.plugin.kotlin.ElideKotlinJVMCompileMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>true</threadSafe>
      <parameters>
        <parameter>
          <name>apiVersion</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>args</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description>Additional command line arguments for Kotlin compiler.</description>
        </parameter>
        <parameter>
          <name>classpath</name>
          <type>java.util.List</type>
          <required>true</required>
          <editable>false</editable>
          <description>Project classpath.</description>
        </parameter>
        <parameter>
          <name>compilerPlugins</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description>A list of kotlin compiler plugins to be applied.</description>
        </parameter>
        <parameter>
          <name>experimentalCoroutines</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>possible values are: enable, error, warn</description>
        </parameter>
        <parameter>
          <name>incrementalCachesRoot</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>javaParameters</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>jdkHome</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>jvmTarget</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>languageVersion</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>module</name>
          <type>java.lang.String</type>
          <deprecated>No reason given</deprecated>
          <required>false</required>
          <editable>false</editable>
          <description>Kotlin compilation module, as alternative to source files or folders.</description>
        </parameter>
        <parameter>
          <name>moduleName</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>myIncremental</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>nowarn</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Suppress all warnings.</description>
        </parameter>
        <parameter>
          <name>output</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>false</editable>
          <description>The directory for compiled classes.</description>
        </parameter>
        <parameter>
          <name>pluginOptions</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description>A list of plugin options in format (pluginId):(parameter)=(value)</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>scriptTemplates</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>session</name>
          <type>org.apache.maven.execution.MavenSession</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>sourceDirs</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description>The source directories containing the sources to be compiled.</description>
        </parameter>
        <parameter>
          <name>testClasspath</name>
          <type>java.util.List</type>
          <required>true</required>
          <editable>false</editable>
          <description>Project test classpath.</description>
        </parameter>
        <parameter>
          <name>testModule</name>
          <type>java.lang.String</type>
          <deprecated>No reason given</deprecated>
          <required>false</required>
          <editable>false</editable>
          <description>Kotlin compilation module, as alternative to source files or folders (for tests).</description>
        </parameter>
        <parameter>
          <name>testModuleName</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>testOutput</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>false</editable>
          <description>The directory for compiled tests classes.</description>
        </parameter>
        <parameter>
          <name>mojoExecution</name>
          <type>org.apache.maven.plugin.MojoExecution</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
      </parameters>
      <configuration>
        <apiVersion implementation="java.lang.String">${kotlin.compiler.apiVersion}</apiVersion>
        <classpath implementation="java.util.List" default-value="${project.compileClasspathElements}"/>
        <experimentalCoroutines implementation="java.lang.String">${kotlin.compiler.experimental.coroutines}</experimentalCoroutines>
        <incrementalCachesRoot implementation="java.lang.String" default-value="${project.build.directory}/kotlin-ic">${kotlin.compiler.incremental.cache.root}</incrementalCachesRoot>
        <javaParameters implementation="boolean">${kotlin.compiler.javaParameters}</javaParameters>
        <jdkHome implementation="java.lang.String">${kotlin.compiler.jdkHome}</jdkHome>
        <jvmTarget implementation="java.lang.String">${kotlin.compiler.jvmTarget}</jvmTarget>
        <languageVersion implementation="java.lang.String">${kotlin.compiler.languageVersion}</languageVersion>
        <moduleName implementation="java.lang.String" default-value="${project.artifactId}"/>
        <myIncremental implementation="boolean" default-value="false">${kotlin.compiler.incremental}</myIncremental>
        <nowarn implementation="boolean" default-value="false"/>
        <output implementation="java.lang.String" default-value="${project.build.outputDirectory}"/>
        <project implementation="org.apache.maven.project.MavenProject" default-value="${project}"/>
        <scriptTemplates implementation="java.util.List">${kotlin.compiler.scriptTemplates}</scriptTemplates>
        <session implementation="org.apache.maven.execution.MavenSession" default-value="${session}"/>
        <testClasspath implementation="java.util.List" default-value="${project.testClasspathElements}"/>
        <testModuleName implementation="java.lang.String" default-value="${project.artifactId}-test"/>
        <testOutput implementation="java.lang.String" default-value="${project.build.testOutputDirectory}"/>
        <mojoExecution implementation="org.apache.maven.plugin.MojoExecution" default-value="${mojoExecution}"/>
      </configuration>
      <requirements>
        <requirement>
          <role>org.codehaus.plexus.PlexusContainer</role>
          <field-name>container</field-name>
        </requirement>
        <requirement>
          <role>org.apache.maven.repository.RepositorySystem</role>
          <field-name>system</field-name>
        </requirement>
      </requirements>
    </mojo>
  </mojos>
  <dependencies>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-maven-plugin</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.codehaus.plexus</groupId>
      <artifactId>plexus-component-annotations</artifactId>
      <type>jar</type>
      <version>2.1.0</version>
    </dependency>
    <dependency>
      <groupId>org.codehaus.plexus</groupId>
      <artifactId>plexus-utils</artifactId>
      <type>jar</type>
      <version>3.5.1</version>
    </dependency>
    <dependency>
      <groupId>org.apache.maven.plugin-tools</groupId>
      <artifactId>maven-plugin-annotations</artifactId>
      <type>jar</type>
      <version>3.7.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-build-tools-api</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-build-tools-impl</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-stdlib</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains</groupId>
      <artifactId>annotations</artifactId>
      <type>jar</type>
      <version>13.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-compiler-runner</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-daemon-client</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-scripting-compiler-impl-embeddable</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-compiler-embeddable</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-script-runtime</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-reflect</artifactId>
      <type>jar</type>
      <version>1.6.10</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-daemon-embeddable</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlinx</groupId>
      <artifactId>kotlinx-coroutines-core-jvm</artifactId>
      <type>jar</type>
      <version>1.8.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-scripting-compiler-embeddable</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-scripting-common</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-scripting-jvm</artifactId>
      <type>jar</type>
      <version>2.2.0</version>
    </dependency>
    <dependency>
      <groupId>org.apache.maven.plugins</groupId>
      <artifactId>maven-compiler-plugin</artifactId>
      <type>jar</type>
      <version>3.10.1</version>
    </dependency>
    <dependency>
      <groupId>org.apache.maven.shared</groupId>
      <artifactId>maven-shared-utils</artifactId>
      <type>jar</type>
      <version>3.3.4</version>
    </dependency>
    <dependency>
      <groupId>org.apache.maven.shared</groupId>
      <artifactId>maven-shared-incremental</artifactId>
      <type>jar</type>
      <version>1.1</version>
    </dependency>
    <dependency>
      <groupId>org.codehaus.plexus</groupId>
      <artifactId>plexus-java</artifactId>
      <type>jar</type>
      <version>1.1.1</version>
    </dependency>
    <dependency>
      <groupId>org.ow2.asm</groupId>
      <artifactId>asm</artifactId>
      <type>jar</type>
      <version>9.2</version>
    </dependency>
    <dependency>
      <groupId>com.thoughtworks.qdox</groupId>
      <artifactId>qdox</artifactId>
      <type>jar</type>
      <version>2.0.1</version>
    </dependency>
    <dependency>
      <groupId>org.codehaus.plexus</groupId>
      <artifactId>plexus-compiler-api</artifactId>
      <type>jar</type>
      <version>2.11.1</version>
    </dependency>
    <dependency>
      <groupId>org.codehaus.plexus</groupId>
      <artifactId>plexus-compiler-manager</artifactId>
      <type>jar</type>
      <version>2.11.1</version>
    </dependency>
    <dependency>
      <groupId>org.codehaus.plexus</groupId>
      <artifactId>plexus-compiler-javac</artifactId>
      <type>jar</type>
      <version>2.11.1</version>
    </dependency>
    <dependency>
      <groupId>commons-io</groupId>
      <artifactId>commons-io</artifactId>
      <type>jar</type>
      <version>2.2</version>
    </dependency>
    <dependency>
      <groupId>com.sun</groupId>
      <artifactId>tools</artifactId>
      <type>jar</type>
      <version>1.8.0</version>
    </dependency>
  </dependencies>
</plugin>
